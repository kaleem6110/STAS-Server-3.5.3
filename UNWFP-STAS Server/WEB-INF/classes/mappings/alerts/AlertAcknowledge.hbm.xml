<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping
    package="com.wfp.db.platform.model">
    <class name="AlertAcknowledgment" table="STI_P_CUSTOM_ACKNOWLEDGMENT">
       <id name="id" type="long">
			<generator class="native" />
		</id>
         
        <property name="message" type="string" column="MESSAGE" />
         <property name="sourceName" type="string" column="SOURCENAME"/>
         <property name="priority" type="string" column="PRIORITY"/>
         <property name="eventDate" type="timestamp" column="EVENT_DATE"/> 
         <property name="eventId" type="string" column="EVENT_ID" />
        
        <property name="comments" type="string" column="COMMENTS"/>
        <property name="eventAckDate" type="timestamp" column="ACK_DATE"/> 
        <property name="ackBy" type="string" column="ACK_BY"/>
        
    </class>
</hibernate-mapping>
