<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" 
"http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>
	<tlibversion>1.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>Self Service</shortname>
	<info>Custom tags for Self Service pages</info>

  <tag>
    <name>GenerateOption</name>
    <tagclass>com.sae.userselfservice.customtags.OptionEngine</tagclass>
    <!--<bodycontent>empty</bodycontent>-->
    <info>This tag generates the various options for a control from the look up table</info>
  <attribute>
      <name>name</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
  </attribute>
    
  <attribute>
      <name>entityName</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
      <name>controlType</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
  </attribute>  
  <attribute>
      <name>defaultValue</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
  </attribute>  

</tag>
</taglib>