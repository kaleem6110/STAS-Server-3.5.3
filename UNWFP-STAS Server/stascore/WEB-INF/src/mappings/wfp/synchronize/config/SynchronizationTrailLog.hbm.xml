<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping
	package="com.wfp.synchronize.db.config.model">

	<class name="SynchronizationTrailLog" table="EPIC_S_SYNC_TRAIL_LOG">
		<id name="id" type="long" column="ID">
            <generator class="native"/>
        </id>
        <property name="userId" type="long" column="USER_ID" />
        <property name="userUniqueId" type="string" column="USER_UNIQUE_ID" />
        <property name="moduleId" type="long" column="MODULE_ID" />
        <property name="syncDataTypeId" column="SYNC_DATA_TYPE_ID" type="long" not-null="true"/>
        <property name="syncUniqueId" column="SYNC_UNIQUE_ID" type="string" />
        <property name="syncTypeId" column="SYNC_TYPE_ID" type="long" />
        <property name="ackId" column="ACK_ID" type="string" />
        <property name="status" column="SYNC_STATUS" type="long" />
        <property name="createdAt" type="string" column="CREATED_AT" />
        
        <property name="createdDate" type="timestamp" column="CREATED_DATE" />
        <property name="modifiedDate" type="timestamp" column="MODIFIED_DATE"/>     
      
    </class>

</hibernate-mapping>