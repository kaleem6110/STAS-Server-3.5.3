table.masterTABLE{
    border: 1px solid black;
    border-collapse: collapse;
    margin:0px;
    padding:0px;
}
table.buttonTABLE{
    border: 1px solid black;
    border-collapse: collapse;
    background:#728893
}
tr.buttonTR{
    text-align:center;
    font-family:Verdana;
    font-size:10pt;
    color:#0b333c;
    font-weight:bold;
    height:20px;
}

input.btnGray{
    background-color:#ddd;
    width: 100%;
    filter:progid:DXImageTransform.Microsoft.Gradient(
	GradientType=0,StartColorStr='#ffffffff',EndColorStr='#ffcccccc');
}

input.btnhov{
    border:1px solid;
    background-color:#fed;
    border-color: #c63 #930 #930 #c63;
    filter:progid:DXImageTransform.Microsoft.Gradient(
	GradientType=0,StartColorStr='#ffffffff',EndColorStr='#ffeeddaa');
}

input.btn{
   color:#0b333c;
   font-family:Verdana,'trebuchet ms',helvetica,sans-serif;
   font-size:8pt;
   font-weight:bold;
   background-color:#acb9bf;
   border:1px solid;
   border-top-color:#b7babc;
   border-left-color:#b7babc;
   border-right-color:#5e6061;
   border-bottom-color:#5e6061;
   width: 100%;
   height: 25px;
   filter:progid:DXImageTransform.Microsoft.Gradient
      (GradientType=0,StartColorStr='#d7dce0ff',EndColorStr='#95a2a9ff');
}

body {
    background: #FFFFFF;
}

