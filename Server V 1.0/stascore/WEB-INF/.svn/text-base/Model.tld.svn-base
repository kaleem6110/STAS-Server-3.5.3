<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
 <tlib-version>1.0</tlib-version>
 <jsp-version>1.2</jsp-version>
 <short-name>RenderModel</short-name>
 <uri>http://con.enterprisehorizons.magma.render.model</uri>
 <tag>
  <name>Render</name>
  <tag-class>com.enterprisehorizons.magma.config.dbadmin.tags.ModelTag</tag-class>
  <body-content>empty</body-content>
  <attribute>
   <name>renderMode</name>
   <required>true</required>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
  <attribute>
   <name>requestScopedModelName</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>themeName</name>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
  <attribute>
   <name>createAction</name>
   <required>false</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
 </tag>
</taglib>
