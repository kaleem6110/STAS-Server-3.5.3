<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping
	package="com.wfp.db.layers.model">

	<class name="RiskZones" table="EPIC_RISK_ZONES">
		<composite-id> 
            <key-property name="eventId" type="string" column="EVENT_ID" length="100" />
        </composite-id>
        
        <property name="countryCd" type="string" column="COUNTRY_GWNO" length="100"/>
		<property name="eventDate" type="timestamp" column="EVENT_DATE" />
		<property name="timePrecision" type="long" column="TIME_PRECISION" />
		<property name="eventType" type="string" column="EVENT_TYPE" />
		<property name="actor1" type="string" column="ACTOR_1" />
		<property name="actorAlly1" type="string" column="ACTOR_ALLY_1" />
		<property name="actor2" type="string" column="ACTOR_2" />
		<property name="actorAlly2" type="string" column="ACTOR_ALLY_2" />
		<property name="country" type="string" column="COUNTRY" />
		<property name="region" type="string" column="REGION" />
		<property name="location" type="string" column="LOCATION" />
		<property name="coordinates" type="string" column="COORDINATES" length="5000"/>	
		<property name="geoprecesion" type="string" column="GEOPRECESION" />
		<property name="publication" type="string" column="PUBLICATION" />
		<property name="notes" type="string" column="NOTES" length="5000"/>
		<property name="actor1Fatalities" type="string" column="ACTOR_1_FATALITIES" />
		<property name="actor2Fatalities" type="string" column="ACTOR_2_FATALITIES" />
		<property name="updatedBy" type="string" column="UPDATED_BY"/>
        <property name="createdDate" type="timestamp" column="CREATED_DATE"/>
        <property name="modifiedDate" type="timestamp" column="MODIFIED_DATE"/>     
        
	</class>

</hibernate-mapping>