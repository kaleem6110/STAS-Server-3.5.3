<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping
	package="com.wfp.synchronize.db.config.model">

	<class name="SynchronizationTrailStatusLog" table="EPIC_S_SYNC_TRAIL_STATUS_LOG">
		<id name="id" type="long" column="ID">
            <generator class="native"/>
        </id>
        <property name="hostname" column="HOSTNAME" type="string" />
        <property name="processedDate" column="SYNC_PROCESS_DATE" type="timestamp" />
        <property name="ackId" column="ACK_ID" type="string" />
        <property name="status" column="SYNC_STATUS" type="long" />
        
        <property name="createdDate" type="timestamp" column="CREATED_DATE" />
        <property name="modifiedDate" type="timestamp" column="MODIFIED_DATE"/>     
      
    </class>

</hibernate-mapping>