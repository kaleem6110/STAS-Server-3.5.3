<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="com.spacetimeinsight.db.scheduler.model">
    <class name="JobTriggerMap" table="STI_JOB_TRIGGER_MAP" >
        <id name="id" type="long">
			<generator class="native" />
		</id>
		<property name="jobId" type="long" column="JOB_ID" not-null="true"/>
		<property name="triggerId" type="long" column="TRIGGER_ID" not-null="true"/>				
    </class>
</hibernate-mapping>
