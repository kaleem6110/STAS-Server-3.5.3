<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wfp.db.platform.model">
	<class name="RegionMessage" table="STI_C_REGION_MESSAGE">
		<id name="id" type="long">
			<generator class="native" />
		</id>

		
		<property name="messageId" type="long" column="MessageId" />
		<property name="rbRegionName" type="string" column="REGION_NAME" />


	</class>
</hibernate-mapping>
