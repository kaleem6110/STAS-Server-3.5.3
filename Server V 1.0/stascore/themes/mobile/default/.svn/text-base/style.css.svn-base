@charset "utf-8";
/* CSS Document */

.bodyBG  	{
				background-color:#FFFFFF;
			}

.bodyText 	{
				font-family:Tahoma, Geneva, sans-serif;
				color:#000;
				font-size:13px;	
	
			}
.bodyText a:link {
				color:#000000;
				text-decoration:none;
				font-size:13px;
			 	}
.bodyText a:visited {
				color:#000000;
				text-decoration:none;
				font-size:13px;
			 }
.formbg {
	            background-color:#f3f2f2;
}
.formlabel {
				text-align:right;
				padding-right:5px;
			}
.formcomponent {
				height:25px;
			}
.selectboxtext{
				font-size:12px;
			}
.btnstyle {
				font-size:13px;
				height:25px;
			}
.headerPadding{
				text-align:right;
				padding-right:5px;
				}
.headerMenuBar {
				background-color:#000000;
				font-family:Verdana, Geneva, sans-serif;
				font-size:16px;
}
.warningtext {
				font-family:Tahoma, Geneva, sans-serif;
				font-size:6px;
				text-align:justify;
				color:#666;
				padding-left:10px;
				padding-right:10px;
			 }
.activelink
			 {
				background-color:#46494a;
			 }
.pageTitle  {
    		 	font-family: Tahoma;
    			font-size: 14px;
    			font-weight: bold;
    			color: #cc092f;
			}
.paddingTitle {
				padding-left:10px;
				padding-top:10px;
				padding-right:10px;
			  }
.paddingTitleDesc {
				font-family: Tahoma;
				font-size: 12px;
				font-weight: bold;
				color: #3d3d3d;
				padding-left:10px; 
			}
					
.hoverbutton {
				background-color:#706f6f;
				border-width: 1px; 
				border-style: Solid; 
				border-color: #5f5e5e; 

			}
.whiteBoldText{
				color:#FFF;
				font-weight:bold;
			}
.hoverText {
				font-family:Verdana, Geneva, sans-serif;
				font-size:12px;
				color:#FFFFFF;
			}
.footerMenuBar {
				font-family:Verdana, Geneva, sans-serif;
				font-size:10.6px;
				
			}
.footerMenuBar a{
				font-family:Verdana, Geneva, sans-serif;
				font-size:10.6px;
				color:#000

			}
.errortext {
				font-family:Verdana, Geneva, sans-serif;
				font-size:10px;
				color:#F00;
			}
	
	
.headerMenu{
				width: 100%;
				height:100%;
				overflow: hidden;
				border-bottom: 1px ;
			}
.headerMenu ul{
				list-style-type: none;
				font-size:12px;
				padding-left:0px;
			}
.headerMenu li{
				display:inline;
				font-size:12px;
			}
.headerMenu li span{
				border-collapse:collapse;
				text-decoration: none;
				padding-left: 5px ;
				padding-right:5px;
				border: 0px ; 
				color:#ebecec;
			}

.headerMenu li span:hover, .headerMenu li.selected span{
				margin:0px;
				font-family:Tahoma;
				font-size:12px;
				font-weight:bold;
				color:#FFF;
				
				cursor:pointer;
				height:100%;
			}