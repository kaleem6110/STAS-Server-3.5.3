/*  popup windows styles */

.overlay_alphacube {
	filter:progid:DXImageTransform.Microsoft.Gradient(endColorstr='#FFFFFF', startColorstr='#cbdeed', gradientType='0');
	filter:alpha(opacity=60);
	-moz-opacity: 0.6;
	opacity: 0.6;
}

.alphacube_nw {
 /* background: transparent url(alphacube/left-top.gif) no-repeat 0 0;			*/
