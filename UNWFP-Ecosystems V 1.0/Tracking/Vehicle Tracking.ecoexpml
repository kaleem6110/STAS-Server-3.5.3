void bindData() 
{
	try 
	{
	 	com.wfp.security.form.DeviceBean data = (com.wfp.security.form.DeviceBean )$trackingdatamodel;	 	
	 	if (data != null) 	
	 	{
	 	  
	 		//Setting the Device ID
	 		$artefact.addAttribute("deviceId", data.getName());
	 		
	 		//Setting the Artifact Co-ordinates
			$artefact.setCoordinates(data.getLatitude(), data.getLongitude());		
			
			//Setting the GMT time 
			//$artefact.addAttribute("time",(Date) com.wfp.utils.CommonUtils.stringDateToDate(data.getTime().replace("T"," ").replace(".000Z","" ) )  );
			//$artefact.addAttribute("time",  (Date)CommonUtils.stringDateToDate(data.getTime(), IEPICConstants.SENSOR_WS_DATE_FORMAT )  );
			$artefact.addAttribute("time",  CommonUtils.parseSensorDate(data.getTime() )  );
			
			//$artefact.addAttribute("pTime",CommonUtils.formatDate((Date)$artefact.getAttributeValue("time") ) );
			$artefact.addAttribute("pTime",  CommonUtils.formatWSDate((Date)$artefact.getAttributeValue("time") ) );
			 
			//Setting the LocalTime
			$artefact.addAttribute("localTime", CommonUtils.stringDateToDate(data.getDeviceLocalTime()) );
			$artefact.addAttribute("pLocalTime", data.getDeviceLocalTime() );	
			
			//Setting Vehicle ID
			$artefact.addAttribute("vehicleID", data.getCn());
			
			//Vehicle Description
			$artefact.addAttribute("description", data.getDescription().trim() );
			
			//Vehicle Licenseplate
			$artefact.addAttribute("licensePlate", data.getLicensePlate());
			
			//Buidling the JSON String
			$artefact.addAttribute("json", buildPopupText(data) );
			
			//Setting the Missions of the Vehicle  
			 $artefact.addAttribute("mission", getAllMissionFromList( LDAPUtils.getLDAPUserDtlsMap().get( data.getName() ).getAuthorizedGroupsList() ) );	
			
			//setting the Organization
			$artefact.addAttribute("organisation", data.getOrganization() );
			
			//$artefact.addAttribute("Contact", com.wfp.utils.CommonUtils.getContactDtls(data.getSkypePager(), data.getMobile(), data.getMail()) );
			//$artefact.addAttribute("owner", data.getCn() +" "+(com.enterprisehorizons.util.StringUtils.isNull(data.getSn())?"":data.getSn()));
			//$artefact.addAttribute("Organization", data.getOrganization());
			//$artefact.addAttribute("callSign", data.getCallSign());
			//$artefact.addAttribute("Fleet", data.getFleet());
			//$artefact.addAttribute("Unit", data.getUnit());
			//$artefact.addAttribute("callSign", data.getCallSign());
			//$artefact.addAttribute("type", data.getVehicleType());			
			//$artefact.addAttribute("reg", data.getVehicleReg());
			
		}
	} 
	catch (Exception e){
		e.printStackTrace();
	}
}

/** Convert Attributes to JSON String. ***/
public String buildPopupText(com.wfp.security.form.DeviceBean data)
{
		List<Tab> tabList = new ArrayList<Tab>();
		List<Legend> vehicleLegendList = new ArrayList<Legend>();
		List<Legend> trackingLegendList = new ArrayList<Legend>();
		List<Legend> historicalLegendList = new ArrayList<Legend>();
		Map<String, String> attribMap = new LinkedHashMap<String,String>();
		
		//Creating a Tab
		Tab vehicleTab = new Tab();
		vehicleTab.setName("Vehicle Details");
		
		//Creating a Legend
		Legend vehicleLegend = new Legend();
		vehicleLegend.setName("Details");
		attribMap.put("Vehicle Id", userProfilesList.contains("admin")?"LDAPVEHICLE"+data.getCn():data.getCn() );
		attribMap.put("Vehicle type", "Car" );
		attribMap.put("Organisation", data.getOrganization()!=""?data.getOrganization():"-" );
		attribMap.put("Description", data.getDescription()!=""?data.getDescription():"-" );
		attribMap.put("Licence plate", data.getLicensePlate()!=""?data.getLicensePlate():"-" );
		attribMap.put("Missions", getAllMissionFromList( LDAPUtils.getLDAPUserDtlsMap().get( data.getName() ).getAuthorizedGroupsList() ) );	
		
		vehicleLegend.setAttribMap( attribMap );
		
		vehicleLegendList.add( vehicleLegend );
		 
		attribMap = new LinkedHashMap<String,String>();	
		
		com.wfp.beans.Messaging messaging =  CommonUtils.getMessaging( data.getSkypePager() );
		
		Legend telLegend = new Legend();
		telLegend.setName("Voice");	 
		
		if( messaging.getHf_CallSign()!=null ) attribMap.put("HF Callsign", messaging.getHf_CallSign() );
		if( messaging.getVhf_CallSign()!=null )attribMap.put("VHF Callsign", messaging.getVhf_CallSign() );
		if( messaging.getThuraya()!=null )attribMap.put("Thuraya", messaging.getThuraya() );
		if( messaging.getIridium()!=null )attribMap.put("Iridium", messaging.getIridium() );
		
		
		telLegend.setAttribMap( attribMap );
		
		if( attribMap.size()>0 )vehicleLegendList.add( telLegend );
		
		vehicleTab.setLegendList( vehicleLegendList );
		tabList.add( vehicleTab );
		
		attribMap = new LinkedHashMap<String,String>();
		
		//Creating Tracking Tab
		Tab trackingTab = new Tab();
		trackingTab.setName("Device Details");
		
		Legend deviceLegend = new Legend();
		deviceLegend.setName("This Device");
		attribMap.put("Device Id", userProfilesList.contains("admin")?"LDAPDEVICE"+data.getName():data.getName() );	
		
		deviceLegend.setAttribMap( attribMap );
		trackingLegendList.add( deviceLegend );
		
		attribMap = new LinkedHashMap<String,String>();
		
		Legend trackingLegend = new Legend();
		trackingLegend.setName("Last Known Position");
		attribMap.put("Position", data.getLatitude() +" / "+data.getLongitude() );
		attribMap.put("reported at", CommonUtils.formatDate((Date)$artefact.getAttributeValue("time") )+" (UTC)" );
		String offs = deviceOffsetMap.get( data.getName() );
		attribMap.put("local time", $artefact.getAttributeValueAsString("pLocalTime") +" (UTC+"+offs+")" );
		
		
		trackingLegend.setAttribMap( attribMap );
		trackingLegendList.add( trackingLegend );
		////////////////////////////////////
		
		/////////////////////////////////////
		attribMap = new LinkedHashMap<String,String>();
		
		Legend historicalLegend = new Legend();
		historicalLegend.setName("PLUSHistorical Tracking");
		
		String mission = $artefact.getAttributeValueAsString("mission");	
		List<String> deviceMissionList = LDAPUtils.getLDAPUserDtlsMap().get( data.getName() ).getAuthorizedGroupsList();
		String miss= "PK";
		if( deviceMissionList!=null && deviceMissionList.size()>0 ) miss = deviceMissionList.get(0);	
		attribMap.put("first reported","oldestDELIM"+data.getCn()+"DELIMVEHICLEDELIM"+miss );
		//attribMap.put("To ( New )", $artefact.getAttributeValueAsString("pTime") );
		attribMap.put("total waypoints", "totalWaypointsDELIM"+data.getUid()+"DELIMVEHICLEDELIM"+miss );
		attribMap.put("days covered", $artefact.getAttributeValueAsString("pTime") );	
		 
		historicalLegend.setAttribMap( attribMap );	
		trackingLegendList.add( historicalLegend );  
		
		
		attribMap = new LinkedHashMap<String,String>();
	
		Legend otherDevicesLegend = new Legend();
		otherDevicesLegend.setName("PLUSOther devices assigned to "+data.getCn() );
		attribMap.put("Radio","N/A");		
		otherDevicesLegend.setAttribMap( attribMap );	
		//trackingLegendList.add( otherDevicesLegend );  
	
		
		//adding legends to tab.
		trackingTab.setLegendList( trackingLegendList );
		
		//adding tab to tabList
		tabList.add( trackingTab );
		

 		 return CommonUtils.parseJsonString( tabList );
 }
