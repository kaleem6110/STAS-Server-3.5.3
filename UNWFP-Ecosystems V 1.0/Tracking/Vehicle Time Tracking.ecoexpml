        <section name="scriptor">
            <section name="body">
                <value><![CDATA[import lu.hitec.pss.soap.sensor.client._12_x.LocationValue;
import com.enterprisehorizons.magma.server.util.ServerUtils;
import com.enterprisehorizons.magma.server.util.Cache;
import com.wfp.beans.*;
import com.wfp.utils.CommonUtils;

String serverRootUrl = ServerUtils.getServerBaseUrl();
com.wfp.security.form.DeviceBean data = null;
private com.enterprisehorizons.magma.designtime.artifact.GeoArtifact $artefact;
private com.wfp.custom.datasource.CustomSoapServiceDataDriver $timetrackingdriver;
String description = "<p>Soap Tracking job which is scheduled for every eight hours fetches data from  <i>https://middleware.service.emergency.lu/sensorservice/out/soap</i> & caches the data. </p> ";
String datasource = "Hitec middleware SOAP service & LDAP ";
String lastRefreshTime = com.enterprisehorizons.util.StringUtils.isNull(com.wfp.jobs.SoapTrackingJob.getInstance().getLastRefreshTime())?"Job not refreshed":com.wfp.jobs.RestTrackingJob.getInstance().getLastRefreshTime() ;

String getDatasource(){
	return datasource;   
}    
 
String getLastRefreshTime(){
	return lastRefreshTime;
}

String getDescription(){
	return description;
}
static {
	if(!Cache.contains("$colorCache$")){
		com.enterprisehorizons.magma.server.util.Cache.store("$colorCache$", new HashMap());
	}
}
void bindData() {

try
	{
	 	com.wfp.security.form.DeviceBean data = (com.wfp.security.form.DeviceBean)$timetrackingdatamodel;
	 	
	 	if (data != null)
	 	{
			$artefact.setCoordinates(data.getCoordStr());
			//changed for getting type as Date instead of String for time
			$artefact.addAttribute("time", (Date) com.wfp.utils.CommonUtils.stringDateToDate(data.getTime().replace("T"," ").replace(".000Z","" ) )  );
			
			   
			$artefact.addAttribute("device", data.getUid());       
			$artefact.addAttribute("name", data.getName());
			$artefact.addAttribute("type", data.getVehicleType());
			$artefact.addAttribute("description", data.getDescription());
			$artefact.addAttribute("licensePlate", data.getLicensePlate());
			
			java.util.Date localTime = com.wfp.utils.CommonUtils.stringDateToDate(data.getDeviceLocalTime());
			//$artefact.addAttribute("localTime", localTime );	
			//$artefact.addAttribute("Timestamp-Local", data.getDeviceLocalTime() );
			$artefact.addAttribute("json", buildPopupText(data) );		
		} 
	}
	catch (Exception e){  
		e.printStackTrace();
	}
	
	
}
 public String buildPopupText(com.wfp.security.form.DeviceBean data)
{
List<Tab> tabList = new ArrayList<Tab>();
List<Legend> vehicleLegendList = new ArrayList<Legend>();
List<Legend> trackingLegendList = new ArrayList<Legend>();

Map<String, String> attribMap = new LinkedHashMap<String,String>();

//Creating a Tab
Tab vehicleTab = new Tab();
vehicleTab.setName("Vehicle");

//Creating a Legend
Legend vehicleLegend = new Legend();
vehicleLegend.setName("Vehicle Details");
attribMap.put("Vehicle Id",data.getName() );
attribMap.put("Vehicle type", "Car" );
attribMap.put("Description",data.getDescription() );
attribMap.put("Licence plate", data.getLicensePlate()!=""?data.getLicensePlate():"-" );



vehicleLegend.setAttribMap( attribMap );

vehicleLegendList.add( vehicleLegend );
 
attribMap = new LinkedHashMap<String,String>();
 

vehicleTab.setLegendList( vehicleLegendList );
tabList.add( vehicleTab );

attribMap = new LinkedHashMap<String,String>();

//Creating Tracking Tab
Tab trackingTab = new Tab();
trackingTab.setName("Tracking");

Legend deviceLegend = new Legend();
deviceLegend.setName("This Device");
attribMap.put("Device Id", data.getCn() );


deviceLegend.setAttribMap( attribMap );
trackingLegendList.add( deviceLegend );

attribMap = new LinkedHashMap<String,String>();

Legend trackingLegend = new Legend();
trackingLegend.setName("Last Known Position");
attribMap.put("Timestamp(GMT)", CommonUtils.formatDate((Date)$artefact.getAttributeValue("time") ) );
attribMap.put("Timestamp( Local)", "localTime" );
attribMap.put("Latitude", data.getLatitude() );
attribMap.put("Longitude", data.getLongitude() );

trackingLegend.setAttribMap( attribMap );
trackingLegendList.add( trackingLegend );

//adding legends to tab.
trackingTab.setLegendList( trackingLegendList );

//adding tab to tabList
tabList.add( trackingTab );



  return CommonUtils.parseJsonString( tabList );
 }

void addAttribute(){
	if(getName().startsWith("trackMe")){
		com.wfp.security.form.LDAPUserBean ldapUserBean = com.wfp.utils.LDAPUtils.getLDAPUserBean(getName());
		if(ldapUserBean == null){
			return;
		}
		
		if(!com.enterprisehorizons.util.StringUtils.isNull(ldapUserBean.getCn()) && !("NULL".equalsIgnoreCase(ldapUserBean.getCn()))){
			$artefact.addAttribute("owner", ldapUserBean.getCn() +" "+(com.enterprisehorizons.util.StringUtils.isNull(ldapUserBean.getSn())?"":ldapUserBean.getSn()));
		}
		if(!StringUtils.isNull(ldapUserBean.getFleet())){
			$artefact.addAttribute("Fleet", ldapUserBean.getFleet());
		}
		
		if(!StringUtils.isNull(ldapUserBean.getUnit())){
			$artefact.addAttribute("Unit", ldapUserBean.getFleet());
		}
			
		if(!StringUtils.isNull(ldapUserBean.getCallSign())){
			$artefact.addAttribute("callSign", ldapUserBean.getCallSign());
			$artefact.addAttribute("type", ldapUserBean.getVehicleType());
			$artefact.addAttribute("reg", ldapUserBean.getVehicleReg());
		}
	}
}

String getDeviceCaption(){	
	if(!com.enterprisehorizons.util.StringUtils.isNull($artefact.getAttributeValueAsString("Unit"))){
		return $artefact.getAttributeValueAsString("Unit") +" " +$artefact.getAttributeValueAsString("Fleet");
	}
	return  $artefact.getAttributeValueAsString("name");
}

String getName() {
	return $artefact.getAttributeValueAsString("name");
}

double getAltitude(){
	data = (com.wfp.security.form.DeviceBean)$timetrackingdatamodel;
	lu.hitec.pss.soap.sensor.client._12_x.LocationValue l = data.getLocationValue();
	
	return l.getAlt();
}

double getLat(){
	data = (com.wfp.security.form.DeviceBean)$timetrackingdatamodel;
	lu.hitec.pss.soap.sensor.client._12_x.LocationValue l = data.getLocationValue();
	
	return l.getLat();
}

double getLng(){
	data = (com.wfp.security.form.DeviceBean)$timetrackingdatamodel;
	lu.hitec.pss.soap.sensor.client._12_x.LocationValue l = data.getLocationValue();
	
	return l.getLng();
}

String getIconUrl(){
	return "serverresources/icons/epic/Vehicle_Time_Tracking.png";
}

String getIconColor(){
	return "#FFFFFF";
}

String getGroupby(){
	return "name";
}

boolean authorizeDevice(){
	com.spacetimeinsight.security.bean.UserBean userbean =  getUserBean();
	return com.wfp.utils.LDAPUtils.authorizeDevices($artefact.getAttributeValueAsString("name"), userbean.getUserUniqueId());
}

String getAuthorUrl(){	
	return serverRootUrl+"/CustomView.jsp?layername="+getLayerName()+"&Datasource ="+datasource+"&Description="+description+"&Last Refresh Time="+lastRefreshTime;
}

String getLineColor(){
	try{
	
		com.wfp.security.form.DeviceBean data = (com.wfp.security.form.DeviceBean)$timetrackingdatamodel;
		String deviceId =getName();
	 	if(deviceId!=null && com.enterprisehorizons.magma.server.util.Cache.contains("$colorCache$")  ){
	 		Map map = (HashMap) com.enterprisehorizons.magma.server.util.Cache.retrieve("$colorCache$");
	 		String color = (String) map.get(deviceId);
	 	
	 		if(color == null){ 					
				color =  "#"+com.enterprisehorizons.util.ColorUtils.getRandomColor();
				while(color.length() != 7){
					color =  "#"+com.enterprisehorizons.util.ColorUtils.getRandomColor();
				}
				map.put(deviceId, color);
		 	}
		 	return color;
		 }
	 }catch(Exception e){
	 	e.printStackTrace();
	}
	return "#FF0000";
}]]></value>
